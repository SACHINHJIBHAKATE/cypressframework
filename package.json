{
  "name": "cypressautomation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node_modules\\.bin\\cypress run",
    "test_headed": "npm run test -- --headed",
    "test_chrome": "npm run test_headed -- --browser chrome",
    "test_onespecfile": "npm run test_headed -- --spec cypress/integration/examples/Test8Framework.js",
    "test_edge": "npm run test_headed -- --browser edge",
    "test_record": "npm run test_headed -- --record --key 26cda711-9a3d-475c-a30b-8d9a1f42cfc8",
    "test_record_onespecfile": "npm run test_onespecfile -- --record --key 26cda711-9a3d-475c-a30b-8d9a1f42cfc8",
    "test_multiplespecfiles_all_files_in_a_folder": "npm run test_headed -- --spec \"cypress/integration/Greenkart/*\"",
    "test_CucumberBDD_Featurefile": "npm run test -- --spec cypress/integration/examples/BDD/ecommerce.feature --headed --browser chrome --record --key 26cda711-9a3d-475c-a30b-8d9a1f42cfc8",
    "test_CucumberBDD_Featurefile_parallel": "npm run test -- --spec cypress/integration/examples/BDD/ecommerce.feature --headed --browser chrome --record --key 26cda711-9a3d-475c-a30b-8d9a1f42cfc8 --parallel"

  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^10.3.0",
    "CYPRESS_CACHE_FOLDER": "~/.cache/Cypress",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-iframe": "^1.0.1",
    "cypress-xpath": "^2.0.1",
    "multiple-cucumber-html-reporter": "^1.21.4"
  }
}
